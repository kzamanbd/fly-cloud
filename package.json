{
    "name": "fly-cloud",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "packageManager": "pnpm@8.15.6",
    "scripts": {
        "build": "turbo build",
        "dev": "turbo dev",
        "lint": "turbo lint",
        "ssh:start": "pnpm --filter=ssh-server start",
        "format": "prettier --write \"**/*.{ts,tsx}\"",
        "artisan": "pnpm --filter=dns-portal exec php artisan"
    },
    "keywords": [
        "laravel",
        "react",
        "express",
        "socket.io"
    ],
    "author": "Kamruzzaman",
    "license": "MIT",
    "devDependencies": {
        "@repo/eslint-config": "workspace:*",
        "@repo/typescript-config": "workspace:*",
        "prettier": "^3.3.3",
        "turbo": "^2.0.12",
        "typescript": "^5.5.4"
    }
}
